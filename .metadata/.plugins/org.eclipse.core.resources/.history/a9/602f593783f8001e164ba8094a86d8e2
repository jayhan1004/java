package am;

import java.util.*;

public class Car { // 자바 빈(java bean)
	//필드(속성) - 브랜드명, 모델, 색, 주행거리, 연식
	public int no;
	private String company;
	private String model;
	private String color;
	private int distance;
	private String date;
	//private 쓰면 밖에서 데이터 못바꿈
	
	//기본생성자(오버로딩 ㅋㅋ)
	public Car() {}
	
	//생성자
	public Car(int no, String company, String model, String color, int distance, String date) {
		super();
		this.no = no;
		this.company = company;
		this.model = model;
		this.color = color;
		this.distance = distance;
		this.date = date;
	}
	
	//getter and setter
	public int getNo() {
		return no;
	}
	public void setNo(int no) {
		this.no = no;
	}
	public String getCompany() {
		return company;
	}
	public void setCompany(String company) {
		this.company = company;
	}
	public String getModel() {
		return model;
	}
	public void setModel(String model) {
		this.model = model;
	}
	public String getColor() {
		return color;
	}
	public void setColor(String color) {
		this.color = color;
	}
	public int getDistance() {
		return distance;
	}
	public void setDistance(int distance) {
		this.distance = distance;
	}
	public String getDate() {
		return date;
	}
	public void setDate(String date) {
		this.date = date;
	}
	@Override
	public String toString() {
		return "Car [no=" + no + ", company=" + company + ", model=" + model + ", color=" + color + ", distance="
				+ distance + ", date=" + date + "]";
	}

	

	
	
	
	
	

		
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	

